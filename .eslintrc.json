{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "@rocketseat/eslint-config/node"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-useless-constructor": "off",
    "no-use-before-define": "off",
    "no-undef": "off",
    "n/handle-callback-err": "off",
    "n/no-path-concat": "off",
    "no-unused-vars": "off"
  }
}